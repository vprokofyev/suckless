!-----------------------------------------------------------------------------
! Init plugins
!-----------------------------------------------------------------------------

Rxvt.perl-ext-common: default,matcher,confirm-paste,selection-to-clipboard

! open urls
!Rxvt.url-launcher: /usr/bin/xdg-open
!Rxvt.matcher.button: 1

! Font-size
!Rxvt.keysym.C-Up:     font-size:increase
!Rxvt.keysym.C-Down:   font-size:decrease
!Rxvt.keysym.C-S-Up:   font-size:incglobal
!Rxvt.keysym.C-S-Down: font-size:decglobal
!Rxvt.keysym.C-equal:  font-size:reset
!Rxvt.keysym.C-slash:  font-size:show
