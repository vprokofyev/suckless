!-----------------------------------------------------------------------------
! Spacing
!-----------------------------------------------------------------------------

Rxvt.letterSpace: -2
Rxvt.lineSpace: 3


!-----------------------------------------------------------------------------
! Spacing
!-----------------------------------------------------------------------------

URxvt.font: xft:SauceCodePro Nerd Font:regular:size=12
URxvt*boldFont: xft:SauceCodePro Nerd Font:medium:size=12
!URxvt.font: xft:SourceCodeProForPowerline-Regular:regular:size=12

!URxvt.font: xft:CourierPrimeCode:regular:size=12

!URxvt.font: xft:SauceCodePro Nerd Font Mono:regular:size=11
!URxvt.font: xft:SauceCodePro Nerd Font Mono:medium:size=11

!URxvt.font: xft:Anonymice Nerd Font Mono:size=11
!URxvt*boldFont: xft:Anonymice Nerd Font Mono:bold:size=11
!URxvt*italicFont: xft:Anonymice Nerd Font Mono:italic:size=11

!URxvt.font: xft:Iosevka Term SS07:light:size=11
!URxvt*font: xft:Terminus:size=12


!-----------------------------------------------------------------------------
! DPI
!-----------------------------------------------------------------------------

Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

